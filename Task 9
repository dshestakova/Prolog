/*removing duplicate elements */

member(X,[X|_]). 		%Проверка, есть ли элемент X в листе
member(X,[_|T]):-
    member(X, T).

del([],[]). 			%Случай, когда список пустой
del([H|T],A):-			%Если X лежит в хвосте, то продолжаем идти по листу
    member(H,T), !,
    del(T,A).
del([H|T],[H|T_]):- 	%Если нет, то сохраняем его в новый список
	del(T,T_). 

